# Biomni Configuration
BIOMNI_DATA_PATH=./data
BIOMNI_TIMEOUT_SECONDS=700
BIOMNI_LLM_MODEL=MODEL_PLACEHOLDER  # Replace with your actual model name
BIOMNI_USE_TOOL_RETRIEVER=true
BIOMNI_BASE_URL=URL_PLACEHOLDER  # Replace with your actual base URL
BIOMNI_API_KEY=KEY_PLACEHOLDER  # Replace with your actual API key

# UI Configuration
SESSION_DATA_PATH=./sessions
LOG_LEVEL=DEBUG
CHAINLIT_PORT=8001
CHAINLIT_HOST=0.0.0.0

# Response Parsing Configuration
ENABLE_SEMANTIC_PARSING=true
ENABLE_SEMANTIC_FIELD_DISPLAY=false
SEMANTIC_TOKEN_THRESHOLD=30

# File Upload Configuration
FILE_UPLOAD_ENABLED=true
MAX_FILE_SIZE_MB=10
# ALLOWED_FILE_TYPES=pdf,docx,txt,png,jpg,csv,json,fasta,fastq,bed,vcf  # Override default types

# Logging Configuration
LOG_FORMAT=%(asctime)s - %(name)s - %(levelname)s - %(message)s
LOG_FILE_MAX_BYTES=10485760
LOG_FILE_BACKUP_COUNT=5